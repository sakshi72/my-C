#include <stdio.h>

int main()
{
    int n=3;
    for(int k=1;k<=n;k++){
    for(int i=1;i<=5;i++){
        for(int j=0;j<i-1;j++){
        printf(" ");
        }
        printf("*****\n");
    }
    for(int i=4;i>=2;i--){
    for(int j=0;j<i-1;j++){
        printf(" ");
        }
        printf("*****\n");
    }
}
    return 0;
}
